{
    "name": "lukaszzychal/php-compatibility-tester",
    "description": "Universal, reusable Composer package for testing PHP library compatibility with various frameworks and PHP versions",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lukasz Zychal",
            "email": "lukasz.zychal.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/console": "^6.0|^7.0",
        "symfony/process": "^6.0|^7.0",
        "symfony/yaml": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "vimeo/psalm": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "LukaszZychal\\PhpCompatibilityTester\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LukaszZychal\\PhpCompatibilityTester\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/compatibility-tester"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}

